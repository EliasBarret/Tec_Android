<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="globalsoft.com.myapplication" >

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/my_globe_im"
        android:label="@string/app_name" >
        <activity
            android:name=".MenuTela"
            android:label="@string/title_activity_menu_tela"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".tela2"
            android:label="@string/title_activity_tela2"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".ExeCalcA"
            android:label="@string/title_activity_exe_calc"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".CalculadoraAV1"
            android:configChanges="orientation"
            android:label="@string/title_activity_calculadora_av1"
            android:parentActivityName=".MenuTela"
            android:screenOrientation="portrait"
            android:theme="@style/Calculator" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="globalsoft.com.myapplication.MenuTela" />
        </activity>
        <activity
            android:name=".Autenticacao"
            android:label="@string/title_activity_autenticacao"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".Expre_calc"
            android:label="@string/title_activity_expre_calc"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".ImagemTelaJava"
            android:label="@string/title_activity_imagem_tela_java"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".CalculadoraSimples"
            android:label="@string/title_activity_calculadora_simples"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".activity_fadein"
            android:label="@string/title_activity_activity_fadein" >
        </activity>
        <activity
            android:name=".AnimcaoRB"
            android:label="@string/title_activity_animcao_rb" >
        </activity>
    </application>

</manifest>
