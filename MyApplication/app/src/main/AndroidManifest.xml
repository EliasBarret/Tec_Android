<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="globalsoft.com.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/my_globe_im"
        android:label="@string/app_name"
        >
        <activity
            android:name=".MenuTela"
            android:label="@string/title_activity_menu_tela"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".tela2"
            android:label="@string/title_activity_tela2"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".ExeCalcA"
            android:label="@string/title_activity_exe_calc"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".CalculadoraAV1"
            android:label="@string/title_activity_calculadora_av1"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:parentActivityName=".MenuTela"
            android:theme="@style/Calculator">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="globalsoft.com.myapplication.MenuTela" />
        </activity>

    </application>
    <uses-permission android:name="android.permission.VIBRATE"/>
</manifest>
